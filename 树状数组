struct BIT
{
    int tree[N] = {0};
    void update (int i,int x)
    {
        for(int pos = i;pos < N;pos += lowbit(pos))
        {
            tree[pos] += x;
        }
    }
    int query(int n)
    {
        int ans = 0;
        for(int pos = n;pos;pos -= lowbit(pos))
            ans += tree[pos];
        return ans;
    }
}
